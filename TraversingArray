class Array Pro2{
public static void m(String value){
String[] s
=
value.split("");
for (int index=0; index<s.length
; index++) {
System.out.println(s[index]
);
}
}
}
Class Array_Pro2 Test{
public static void main(String
..abc){
Array_Pro2.m("JAVALAYA");
}
}

class Array_Pr03{
public static void m(String value){
char[] c
=
value. toCharArray();
for (int index=0; index<c.length
: index++){
System.out.println(c[index]
):
}
}
}
class Array_Pro3_Test{
public static void main(String
.abc){
Array_Pro3.m ("JAVALAYA");
}
}

